module ql/sort

imports
  include/QL
  lib/relations/-
  lib/index/-
  lib/task/-
  lib/nabl/-
  
rules
  
  question-name: |[ if (c) { qid: label type  } ]| -> qid
  question-name: |[ if (c) { qid: label texpr } ]| -> qid
  
  depends-on(|q1) =
	  qid2 := <question-name> 
  ; <question-name; relation-match(|"<depends-on:", qid2)> q1
	  
  sort-by-dependencies = Form(id, quick-sort(depends-on))

  