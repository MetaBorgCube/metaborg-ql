module QL

imports

  Expressions
  Types
	Lexical
	
context-free start-symbols

	Start

templates // top-level

	Start.Form = <
		form <ID> {
			<Group*; separator="\n\n">
		}
	>
	
templates // questions

	Group.Question = <
		<ID> : <Label> <Type>
	>
	
	Group.Computed = <
		<ID> : <Label> <TyExpr>
	>
	
	Label.Label = <<STRING>>
	
templates // conditional

	Group.Conditional = <
		if (<Expr>) {
			<Group*; separator="\n\n">
		}
	>
