templates // top-level

	Form.Form = <
		form <ID> {
			<Group*; separator="\n\n">
		}
	>
	
templates // questions
	
	Group = <<Question>>

	Question.Question = <
		<ID> : <Label> <Type>
	>
	
	Question.Computed = <
		<ID> : <Label> <TyExpr>
	>
	
	Label.Label = <<STRING>>
	
templates // conditional

	Group.Conditional = <
		if (<Expr>) {
			<Group*; separator="\n\n">
		}
	>
	
templates // types

	Type.BoolTy = <boolean>
	Type.StringTy = <string>
	Type.IntTy = <integer>
	Type.DateTy = <date>
	Type.MoneyTy = <money>

templates // expressions
	Expr.Bar = <bar>
	TyExpr.Foo = <foo>